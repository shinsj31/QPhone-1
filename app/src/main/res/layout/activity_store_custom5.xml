<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.a502.drawex.store_custom5"
    android:paddingLeft="20dp"
    android:paddingRight="40dp"
    android:paddingBottom="30dp"
    android:background="@drawable/custom_end">
    <com.example.a502.drawex.CouponView
    android:layout_centerVertical="true"
    android:id="@+id/couponView"
    android:layout_marginBottom="50dp"
    android:layout_width="match_parent"
    android:layout_height="100dp" />
    <Button
        android:layout_below="@+id/couponView"
        android:background="@drawable/main_frame"
        android:text="save"
        android:id="@+id/btn_save"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:layout_marginLeft="20dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"/>
</RelativeLayout>
